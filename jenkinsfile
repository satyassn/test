pipeline {
    agent any

    stages {
        stage('Init') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/satyassn/test.git']]])
            }
        }
        stage('Hello') {
            steps {
                echo $GIT_BRANCH
            }
        }
    }
}
